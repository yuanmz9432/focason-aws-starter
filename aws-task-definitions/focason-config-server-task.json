{
  "family": "focason-cloud-config-server-task",
  "containerDefinitions": [
    {
      "cpu": 512,
      "environment": [
        {
          "name": "SERVER_PORT",
          "value": "8888"
        },
        {
          "name": "APPLICATION_NAME",
          "value": "focason-config-server"
        },
        {
          "name": "INSTANCE_HOSTNAME",
          "value": "focason-config-server.focason-cloud.internal"
        },
        {
          "name": "EUREKA_HOSTNAME",
          "value": "focason-service-registry.focason-cloud.internal"
        },
        {
          "name": "RABBITMQ_HOST",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:442426889980:secret:focason-cloud-secret-vOqQte:RABBITMQ_HOST::"
        },
        {
          "name": "RABBITMQ_PORT",
          "value": "5671"
        },
        {
          "name": "RABBITMQ_USERNAME",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:442426889980:secret:focason-cloud-secret-vOqQte:RABBITMQ_USERNAME::"
        },
        {
          "name": "RABBITMQ_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:442426889980:secret:focason-cloud-secret-vOqQte:RABBITMQ_PASSWORD::"
        },
        {
          "name": "RABBITMQ_SSL",
          "value": "true"
        },
        {
          "name": "LOG_LEVEL",
          "value": "INFO"
        },
        {
          "name": "ENVIRONMENT",
          "value": "development"
        }
      ],
      "environmentFiles": [],
      "essential": true,
      "image": "442426889980.dkr.ecr.ap-northeast-1.amazonaws.com/focason-cloud/focason-config-server:latest",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/focason-cloud-services/focason-config-server",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "memory": 1024,
      "memoryReservation": 1024,
      "mountPoints": [],
      "name": "focason-cloud-config-server-container",
      "portMappings": [
        {
          "appProtocol": "http",
          "containerPort": 8888,
          "hostPort": 8888,
          "name": "focason-config-server-8888-tcp",
          "protocol": "tcp"
        }
      ],
      "systemControls": [],
      "ulimits": [],
      "volumesFrom": []
    }
  ],
  "executionRoleArn": "arn:aws:iam::442426889980:role/focason-cloud-ecs-execution-role",
  "taskRoleArn": "arn:aws:iam::442426889980:role/focason-cloud-ecs-task-role",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "enableFaultInjection": false
}